{
  "expo": {
    "name": "sds",
    "slug": "sds",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.unchanted.gymme",
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses the camera to show video for video calls.",
        "NSMicrophoneUsageDescription": "This app uses the microphone to record audio for video calls."
      }
    },
    "android": {
      "config": {
        "googleMaps": {
          "apiKey": "process.env.EXPO_PUBLIC_API_KEY"
        }
      },
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.unchanted.gymme"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": ["expo-router", "expo-font", "expo-video"],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "8e4eb5e6-2f82-461f-a691-daab5e4190b4"
      }
    },
    "owner": "sds-gymme"
  }
}
