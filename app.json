{
  "expo": {
    "name": "sds",
    "slug": "gymmeapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.unchanted.gymme",
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses the camera to show video for video calls.",
        "NSMicrophoneUsageDescription": "This app uses the microphone to record audio for video calls."
      }
    },
    "android": {
      "config": {
        "googleMaps": {
          "apiKey": "process.env.EXPO_PUBLIC_API_KEY"
        }
      },
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.unchanted.gymme"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-video"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "ba0d84dd-2ee2-448c-be15-b832d7a44a8a"
      },
      "router": {
        "origin": false
      }
    },
    "owner": "back_end"
  }
}
